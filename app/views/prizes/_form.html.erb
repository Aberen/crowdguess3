<div class="form-container py-6">
    <%= form_for @prize do |f| %>
        <div class="field">
            <%= f.label :name %><br />
            <%= f.text_field :name %>
        </div>
        <div class="field">
            <%= f.label :key %><br />
            <%= f.text_area :key %>
        </div>

        <div class="actions">
            <%= f.submit %>
        </div>
    <% end %>
</div>